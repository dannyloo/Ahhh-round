<?xml version="1.0" encoding="utf-8"?>
<com.gamestridestudios.ahhh_round.components.SelectSkinListRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/select_skin_text_view_image"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="12dp"
        android:layout_marginLeft="14dp"
        android:layout_gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.gamestridestudios.ahhh_round.components.TextView
        android:id="@+id/select_skin_text_view_predicate_text_view"
        android:layout_marginLeft="12dp"
        android:layout_gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <View
        android:layout_weight="1"
        android:layout_width="wrap_content"
        android:layout_height="match_parent" />
    <com.gamestridestudios.ahhh_round.components.RoundedButton
        android:id="@+id/select_skin_text_view_select_button"
        android:layout_marginRight="12dp"
        android:clickable="false"
        android:focusable="false"
        android:layout_gravity="center_vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</com.gamestridestudios.ahhh_round.components.SelectSkinListRow>